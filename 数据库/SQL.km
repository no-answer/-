                                        ]
                                    },
                                    {
                                        "data": {
                                            "id": "c4jn0etaiuo0",
                                            "created": 1596079636818,
                                            "text": "此时SQL允许执行update、insert、delete操作"
                                        },
                                        "children": []
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "data": {
                            "id": "c4jn0c1fzg80",
