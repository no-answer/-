                                "children": [
                                    {
                                        "data": {
                                            "id": "c4b4q4160q80",
                                            "created": 1595215570076,
                                            "text": "create table r (A1 D1, A2 D2, ..., An Dn, <完整性约束i>, ..., <完整性约束k>);"
                                        },
                                        "children": [
                                            {
                                                "data": {
                                                    "id": "c4b4ra2y9cw0",
                                                    "created": 1595215661609,
                                                    "text": "r为关系名（表名），Ai为属性名，Di是属性Ai的域"
                                                },
                                                "children": []
                                            }
                                        ]
                                    },
                                    {
                                        "data": {
                                            "id": "c4b6ff0eq680",
